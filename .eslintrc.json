{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
      "import"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-prototype-builtins": "off",
      "import/no-unresolved": ["error", {"commonjs": true, "amd": true}]
    }
}
